datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Employee {
  createdAt   DateTime @default(now())
  email       String?
  firstName   String?
  id          String   @id @default(cuid())
  lastName    String?
  phoneNumber String?
  position    String?
  salary      Float?
  updatedAt   DateTime @updatedAt
}
